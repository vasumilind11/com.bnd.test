#
#  APPLICATION
#

Bundle-Version: 1.0.0.${tstamp}
Bundle-Description: \
	This is the  bundle. It carries the configuration for an application \
	and provides some basic very much application specific stuff.  It provides a button \
	that calls a REST function that turns a text into upper case. \
	\
	${warning;Please update this Bundle-Description in com.bnd.test.eval.application/bnd.bnd}

Private-Package: \
	com.bnd.test.eval.application,\
	com.bnd.test.eval.api

EnRoute-Application: 	com.bnd.test.eval
JPM-Command:			eval

-includeresource: \
	{readme.md}, \
	{configuration=configuration}, \
	{static=static}, \
	web=web

-buildpath: \
	osgi.enroute.base.api,\
	com.bnd.test.eval.api;version=latest
	
-testpath: \
	osgi.enroute.junit.wrapper;version=4.12
